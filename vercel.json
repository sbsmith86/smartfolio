{
  "buildCommand": "prisma generate && yarn build",
  "devCommand": "yarn dev",
  "installCommand": "yarn install",
  "redirects": [
    {
      "source": "/auth/:path*",
      "destination": "/dashboard",
      "permanent": true
    }
  ],
  "cleanUrls": true,
  "alias": ["smartfolio.vercel.app"],
  "functions": {
    "app/api/**/*.ts": {
      "includeFiles": "node_modules/.prisma/client/**"
    }
  }
}
